{
    "name": "bachi/typo3-aop",
    "description": "TYPO3 Extension to enable aop programming.",
    "type": "typo3-cms-extension",
    "require": {
        "php": ">=7.1,<7.3",
        "goaop/framework": "^2.1",
        "typo3/cms-core": "^8.7 || ^9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Bachi\\AOP\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bachi\\AOP\\Tests": "Tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Bachmann",
            "email": "markus.bachmann@bachi.biz"
        }
    ],
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
